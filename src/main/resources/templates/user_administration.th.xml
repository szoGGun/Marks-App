<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#addNewUser"
          th:href="${addUser}"
          th:with="addUser=${T(org.przemekrutkowski.exammarksapp.util.Mappings).ADD_USER}"/>
    <attr sel="#deleteUser"
          th:href="${deleteUser}+'?userName='+${user.userName}"
          th:with="deleteUser=${T(org.przemekrutkowski.exammarksapp.util.Mappings).DELETE_USER}"/>
    <attr sel="#editUser"
          th:href="${editUser}+'?userName='+${user.userName}"
          th:with="editUser=${T(org.przemekrutkowski.exammarksapp.util.Mappings).EDIT_USER}"/>
    <attr sel="#home"
          th:href="${home}"
          th:with="home=${T(org.przemekrutkowski.exammarksapp.util.Mappings).HOME}"/>
</thlogic>